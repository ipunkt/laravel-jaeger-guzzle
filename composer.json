{
  "name": "ipunkt/laravel-jaeger-guzzle",
  "description": "Pass ipunkt/laravel-jaeger traces to guzzle requests",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sven Speckmaier",
      "email": "sps@ipunkt.biz"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ipunkt\\LaravelJaegerGuzzle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ipunkt\\LaravelJaegerGuzzleTests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1.3",
    "ipunkt/laravel-jaeger": "^1.1",
    "laravel/framework": "^5|^6"
  },
  "extra": {
    "laravel": {
        "providers": [
            "Ipunkt\\LaravelJaegerGuzzle\\Provider"
        ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1",
    "orchestra/testbench": "^3.8"
  }
}
